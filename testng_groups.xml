<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite">
    <test name="Test">
        <groups>
            <run>
                <!--                include tag will run the @test marked with the name -->
                <!--                exclude tag will not  run the @test marked with the name-->
                <include name="sanity"/>
                <include name="smoke"/>
                <exclude name="regression"/>
            </run>
        </groups>
        <classes>
            <class name="testpacktestngdependsfeature.SampleScript"/>
        </classes>
    </test> <!-- Test -->
</suite> <!-- Suite -->
